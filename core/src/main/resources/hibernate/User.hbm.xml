<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.tw.model.User" table="user" catalog="">
        <id name="userId" type="java.lang.Long">
            <column name="USER_ID" />
            <generator class="identity" />
        </id>
        <property name="userName" type="string">
            <column name="USER_NAME" length="20" not-null="true" unique="true" />
        </property>
        <property name="userEmail" type="string">
            <column name="USER_EMAIL" not-null="true" unique="true" />
        </property>
        <property name="userAge" type="int">
            <column name="USER_AGE" not-null="true" unique="true" />
        </property>
    </class>
</hibernate-mapping>